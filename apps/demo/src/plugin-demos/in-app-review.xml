<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" class="page">
    <Page.actionBar>
        <ActionBar title="in-app-review" icon="" class="action-bar">
        </ActionBar>
    </Page.actionBar>
    <StackLayout class="p-20">
        <ScrollView class="h-full">
          <StackLayout>
            <Button text="Init in-app-review" tap="{{ init }}" class="btn btn-primary"/>
            <Button text="Launch in-app-review" tap="{{ launch }}" class="btn btn-primary"/>
          </StackLayout>
        </ScrollView>
    </StackLayout>
</Page>